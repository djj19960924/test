page{
  background: #E5E5E5;
}
.group{
  margin-bottom: 16rpx;
  background: #fff;
}
.headImgp{
  position: relative;
  width: calc(100% - 15px);
  height: 80px;
  background: #fff;
  padding: 12px 15px 12px 0;
  border-bottom: 1px solid #F5F6F6;
  margin-left: 15px;
}
.headImg{
  width: 50px;
  height: 50px;
  position: absolute;
  right: 15px;
  top: 15px;
  border-radius: 50%;
  z-index: 1000;
  overflow: hidden;
}
.headTitle{
  width: 100%;
  height: 50px;
  line-height: 54px;
  font-size: 14px;
}